{
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ],
      "no-console": [
        2,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "arrow-parens": [
        2,
        "as-needed",
        {
          "requireForBlockBody": true
        }
      ],
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "block-scoped-var": 2,
      "camelcase": 2,
      "computed-property-spacing": [
        2,
        "never"
      ],
      "curly": [
        2,
        "multi-line",
        "consistent"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "eqeqeq": [
        2,
        "smart"
      ],
      "keyword-spacing": 2,
      "max-depth": [
        1,
        3
      ],
      "max-len": [
        1,
        180
      ],
      "max-statements": [
        1,
        40
      ],
      "max-params": [
        1,
        4
      ],
      "new-cap": 1,
      "max-nested-callbacks": [
        2,
        4
      ],
      "no-extend-native": 2,
      "no-else-return": 1,
      "no-lonely-if": 1,
      "no-path-concat": 2,
      "no-unused-vars": [
        2,
        "all"
      ],
      "no-undef": 1,
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "no-floating-decimal": 1,
      "no-mixed-requires": [
        1,
        true
      ],
      "no-useless-escape": 2,
      "no-sync": 0,
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "space-unary-ops": 2,
      "semi": [
        2,
        "always"
      ],
      "no-unneeded-ternary": [2, { "defaultAssignment": false }]
}
